{
  "name": "server",
  "version": "1.0.0",
  "description": "Simple socialmedia website built using MERN Stack Technologies",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gridfs-stream": "^1.1.1",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.12.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bramha007/SocialMERN.git"
  },
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "build": "npm install --prefix server && npm install --prefix client && npm run build --prefix client",
    "build:deploy": "npm install && npm run build"
  },
